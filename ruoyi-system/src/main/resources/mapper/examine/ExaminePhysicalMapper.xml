<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.examine.mapper.ExaminePhysicalMapper">

    <resultMap type="com.ruoyi.examine.domain.vo.ExaminePhysicalVo" id="ExaminePhysicalResult">
        <result property="id" column="id"/>
        <result property="medicalUserId" column="medical_user_id"/>
        <result property="retiredUserId" column="retired_user_id"/>
        <result property="title" column="title"/>
        <result property="content" column="content"/>
        <result property="examineTime" column="examine_time"/>
        <result property="remark" column="remark"/>
        <result property="processStatus" column="process_status"/>
        <result property="status" column="status"/>
        <result property="delFlag" column="del_flag"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <sql id="selectExaminePhysicalVo">
        select id,
               medical_user_id,
               retired_user_id,
               title,
               content,
               examine_time,
               remark,
               process_status,
               status,
               del_flag,
               create_by,
               create_time,
               update_by,
               update_time
        from examine_physical
    </sql>

    <select id="selectExaminePhysicalList" parameterType="com.ruoyi.examine.domain.vo.ExaminePhysicalVo"
            resultMap="ExaminePhysicalResult">
        <include refid="selectExaminePhysicalVo"/>
        <where>
            <if test="medicalUserId != null ">and medical_user_id = #{medicalUserId}</if>
            <if test="retiredUserId != null ">and retired_user_id = #{retiredUserId}</if>
            <if test="title != null  and title != ''">and title = #{title}</if>
            <if test="content != null  and content != ''">and content = #{content}</if>
            <if test="examineTime != null ">and examine_time = #{examineTime}</if>
            <if test="processStatus != null  and processStatus != ''">and process_status = #{processStatus}</if>
            <if test="status != null  and status != ''">and status = #{status}</if>
        </where>
    </select>

    <select id="selectExaminePhysicalById" parameterType="Long" resultMap="ExaminePhysicalResult">
        <include refid="selectExaminePhysicalVo"/>
        where id = #{id}
    </select>

    <insert id="insertExaminePhysical" parameterType="com.ruoyi.examine.domain.ExaminePhysical" useGeneratedKeys="true"
            keyProperty="id">
        insert into examine_physical
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="medicalUserId != null">medical_user_id,</if>
            <if test="retiredUserId != null">retired_user_id,</if>
            <if test="title != null">title,</if>
            <if test="content != null">content,</if>
            <if test="examineTime != null">examine_time,</if>
            <if test="remark != null">remark,</if>
            <if test="processStatus != null">process_status,</if>
            <if test="status != null">status,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="medicalUserId != null">#{medicalUserId},</if>
            <if test="retiredUserId != null">#{retiredUserId},</if>
            <if test="title != null">#{title},</if>
            <if test="content != null">#{content},</if>
            <if test="examineTime != null">#{examineTime},</if>
            <if test="remark != null">#{remark},</if>
            <if test="processStatus != null">#{processStatus},</if>
            <if test="status != null">#{status},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
        </trim>
    </insert>

    <update id="updateExaminePhysical" parameterType="com.ruoyi.examine.domain.ExaminePhysical">
        update examine_physical
        <trim prefix="SET" suffixOverrides=",">
            <if test="medicalUserId != null">medical_user_id = #{medicalUserId},</if>
            <if test="retiredUserId != null">retired_user_id = #{retiredUserId},</if>
            <if test="title != null">title = #{title},</if>
            <if test="content != null">content = #{content},</if>
            <if test="examineTime != null">examine_time = #{examineTime},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="processStatus != null">process_status = #{processStatus},</if>
            <if test="status != null">status = #{status},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteExaminePhysicalById" parameterType="Long">
        delete
        from examine_physical
        where id = #{id}
    </delete>

    <delete id="deleteExaminePhysicalByIds" parameterType="String">
        delete from examine_physical where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>
