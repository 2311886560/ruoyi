<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.examine.mapper.RetiredMapper">

    <resultMap type="com.ruoyi.examine.domain.Retired" id="RetiredResult">
        <result property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="address" column="address"/>
        <result property="unit" column="unit"/>
        <result property="phone" column="phone"/>
        <result property="head" column="head"/>
        <result property="remark" column="remark"/>
        <result property="status" column="status"/>
        <result property="delFlag" column="del_flag"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <sql id="selectRetiredVo">
        select id,
               name,
               address,
               unit,
               phone,
               head,
               remark,
               status,
               del_flag,
               create_by,
               create_time,
               update_by,
               update_time
        from retired
    </sql>

    <select id="selectRetiredList" parameterType="com.ruoyi.examine.domain.Retired" resultMap="RetiredResult">
        <include refid="selectRetiredVo"/>
        <where>
            <if test="name != null  and name != ''">and name like concat('%', #{name}, '%')</if>
            <if test="address != null  and address != ''">and address like concat('%', #{address}, '%')</if>
            <if test="unit != null  and unit != ''">and unit = #{unit}</if>
            <if test="phone != null  and phone != ''">and phone = #{phone}</if>
            <if test="head != null  and head != ''">and head like concat('%', #{head}, '%')</if>
            <if test="status != null  and status != ''">and status = #{status}</if>
        </where>
    </select>

    <select id="selectRetiredById" parameterType="Long" resultMap="RetiredResult">
        <include refid="selectRetiredVo"/>
        where id = #{id}
    </select>

    <insert id="insertRetired" parameterType="com.ruoyi.examine.domain.Retired" useGeneratedKeys="true"
            keyProperty="id">
        insert into retired
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="name != null">name,</if>
            <if test="address != null">address,</if>
            <if test="unit != null">unit,</if>
            <if test="phone != null">phone,</if>
            <if test="head != null">head,</if>
            <if test="remark != null">remark,</if>
            <if test="status != null">status,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="name != null">#{name},</if>
            <if test="address != null">#{address},</if>
            <if test="unit != null">#{unit},</if>
            <if test="phone != null">#{phone},</if>
            <if test="head != null">#{head},</if>
            <if test="remark != null">#{remark},</if>
            <if test="status != null">#{status},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
        </trim>
    </insert>

    <update id="updateRetired" parameterType="com.ruoyi.examine.domain.Retired">
        update retired
        <trim prefix="SET" suffixOverrides=",">
            <if test="name != null">name = #{name},</if>
            <if test="address != null">address = #{address},</if>
            <if test="unit != null">unit = #{unit},</if>
            <if test="phone != null">phone = #{phone},</if>
            <if test="head != null">head = #{head},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="status != null">status = #{status},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteRetiredById" parameterType="Long">
        delete
        from retired
        where id = #{id}
    </delete>

    <delete id="deleteRetiredByIds" parameterType="String">
        delete from retired where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>
